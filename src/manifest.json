{
  "manifest_version": 3,
  "name": "Rui Calendar Selector",
  "version": "1.2.0",
  "description": "grouping calendars for google calendar.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["https://calendar.google.com/calendar/*"],
      "js": ["index.js"],
      "css": ["index.css"]
    }
  ],
  "permissions": ["storage"],
  "web_accessible_resources": [
    {
      "resources": ["*.svg"],
      "matches": ["<all_urls>"]
    }
  ]
}
